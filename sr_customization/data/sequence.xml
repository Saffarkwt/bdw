<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		<record id="seq_customer_invoice" model="ir.sequence">
			<field name="name">Customer Invoice</field>
			<field name="code">customer.invoice</field>
			<field name="prefix"></field>
			<field name="padding">5</field>
			<field name="number_next">12000</field>
            <field name="number_increment">1</field>
			<field name="company_id" eval="False" />
		</record>
		<record id="seq_vendor_bill" model="ir.sequence">
			<field name="name">Vendor Bill</field>
			<field name="code">vendor.bill</field>
			<field name="prefix"></field>
			<field name="padding">5</field>
			<field name="number_next">10000</field>
            <field name="number_increment">1</field>
			<field name="company_id" eval="False" />
		</record>
		<record id="seq_display_invoice" model="ir.sequence">
			<field name="name">Display Invoice</field>
			<field name="code">display.invoice</field>
			<field name="prefix"></field>
			<field name="padding">5</field>
			<field name="number_next">1000</field>
            <field name="number_increment">1</field>
			<field name="company_id" eval="False" />
		</record>
		<record id="seq_return_invoice" model="ir.sequence">
			<field name="name">Return Invoice</field>
			<field name="code">return.invoice</field>
			<field name="prefix"></field>
			<field name="padding">5</field>
			<field name="number_next">100</field>
            <field name="number_increment">1</field>
			<field name="company_id" eval="False" />
		</record>
	</data>
</odoo>
